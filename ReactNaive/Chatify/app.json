{
  "expo": {
    "name": "Chatify",
    "slug": "Chatify",
    "version": "1.0.0",
    "scheme": "chatify",
    "orientation": "portrait",
    "icon": "./assets/Images/ChatIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/Splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
